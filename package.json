{
  "name": "ai-comply",
  "version": "2.0.0",
  "description": "EU AI Act compliance toolkit for Irish SMEs - local-first, no backend required",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write 'src/**/*.{ts,css}' '*.{json,html}'",
    "format:check": "prettier --check 'src/**/*.{ts,css}' '*.{json,html}'",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "dexie": "^4.0.1",
    "file-saver": "^2.0.5",
    "jspdf": "^2.5.2",
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/file-saver": "^2.0.7",
    "prettier": "^3.8.1",
    "typescript": "^5.6.0",
    "vite": "^5.4.0",
    "vitest": "^2.0.0"
  }
}
